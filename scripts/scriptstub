#!/usr/bin/bash
echo "#!/usr/bin/bash"
echo ""
echo "declare -a commands=(funcname)"
echo ""
echo "if [[ -n \$COMP_LINE ]]; then"
echo "    for c in \"\${commands[@]}\"; do"
echo "        [[ \${c:0:\${#2}} == \"\${2,,}\" ]] && echo \"\$c\""
echo "    done"
echo "    exit"
echo "fi"
echo ""
echo "_funcname() {"
echo "    echo \"Write your script!\""
echo "}"
echo ""
echo "declare CMD=\"\$1\"; shift"
echo ""
echo "for c in \"\${commands[@]}\"; do"
echo ""
echo "    [[ \$c == \"\$CMD\" ]] && \"_\$c\" \"\$@\" && exit \$?"
echo ""
echo "done"
